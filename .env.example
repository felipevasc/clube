GEMINI_API_KEY=exemplochave...
GEMINI_API_KEY2=exemplochave...
# Provedor para montar o prompt de estilo (ordem importa: gemini | local)
AI_STYLE_TEXT_PROVIDER_QUEUE=gemini,local
# Modelo Gemini para montar prompt de estilo
GEMINI_STYLE_MODEL=gemini-2.5-flash
# Queue de provedores de imagem (ordem importa, ex.: gemini | openai,gemini)
AI_IMAGE_PROVIDER_QUEUE=gemini
# Modelo Gemini image-to-image ("nano banana" estavel)
GEMINI_IMAGE_MODEL=gemini-2.5-flash-image
HUGGING_FACE_API_KEY=exemplochave...
NVIDIA_API_KEY=exemplochave...
OPENROUTER_API_KEY=sua_chave
OPENAI_API_KEY=exemplochave...
OPENAI_IMAGE_MODEL=gpt-image-1.5
# Primary reference-image model for /v1/images/edits (gpt-image-1-mini | gpt-image-1 | gpt-image-1.5)
OPENAI_IMAGE_TOOL_MODEL=gpt-image-1.5
# /v1/responses is now fallback when /v1/images/edits fails.
OPENAI_RESPONSES_MODEL=gpt-4.1-nano
# Fallbacks if the main Responses model is blocked by org verification/access.
OPENAI_RESPONSES_FALLBACK_MODELS=gpt-4o-mini,gpt-4.1-mini,gpt-4o
OPENAI_IMAGE_TOOL_SIZE=1024x1024
OPENAI_IMAGE_TOOL_QUALITY=medium
OPENAI_IMAGE_TOOL_FORMAT=png
# Optional for Responses image_generation tool.
OPENAI_IMAGE_INPUT_FIDELITY=high
OPENAI_IMAGE_TOOL_ACTION=generate
# Keep false to avoid old-model fallback (dall-e-2).
OPENAI_USE_LEGACY_EDITS_FALLBACK=false
OPENAI_IMAGE_EDIT_FALLBACK_MODEL=dall-e-2
SILICONFLOW_API_KEY=exemplochave...
