{
  "name": "clube",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "workspaces": [
    "packages/*",
    "services/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "node scripts/dev.mjs",
    "setup": "pnpm -r db:setup",
    "dev:setup": "pnpm setup && pnpm dev",
    "build:web": "pnpm --filter @clube/web build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "prisma": "^6.3.1"
  }
}